{
  "extends": "airbnb-base",
  "root": true,
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "import/extensions": ["error", "never", { "js": "always" }]
  }
}
